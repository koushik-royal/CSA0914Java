import java.util.*;

public class KthSmallestElement {
    public static int findKthSmallest(Set<Integer> set, int k) {
        List<Integer> list = new ArrayList<>(set);
        Collections.sort(list);
        return list.get(k - 1);
    }

    public static boolean areSetsEqual(Set<Integer> set1, Set<Integer> set2) {
        return set1.equals(set2);
    }

    public static void main(String[] args) {
        Set<Integer> set = new HashSet<>(Arrays.asList(5, 3, 8, 1, 9, 2));
        int k = 3;
        int kthSmallest = findKthSmallest(set, k);
        System.out.println("The " + k + "-th smallest element is: " + kthSmallest);

        Set<Integer> set1 = new HashSet<>(Arrays.asList(1, 2, 3));
        Set<Integer> set2 = new HashSet<>(Arrays.asList(1, 2, 3));
        System.out.println("Are the sets equal? " + areSetsEqual(set1, set2));
    }
}
